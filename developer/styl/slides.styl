@css {
  @supports ( position: sticky ) {
    article.example.example--support .half.sticky {
      background: green;
    }
    article.example.example--support .half.sticky:after {
      content: "ДА";
      color: #060;
    }
  }

  @supports ( motion-path: path('M 0,0') ) {
    article.example.example--support .half.motion {
      background: green;
    }
    article.example.example--support .half.motion:after {
      content: "ДА";
      color: #060;
    }
  }
}

